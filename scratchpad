SELECT 
  CASE WHEN T1.Label = 'Total' THEN 'FormattedDue' ELSE T1.Label END AS Label,
  T1.'Count 2025-01',
  T1.'Count 2025-02',
  -- add other month columns here
  T1.'Count 2025-12'
FROM T1

UNION ALL

SELECT 
  CASE WHEN T2.Label = 'Total' THEN 'FormattedResolution' ELSE T2.Label END AS Label,
  T2.'Count 2025-01',
  T2.'Count 2025-02',
  -- add other month columns here
  T2.'Count 2025-12'
FROM T2
