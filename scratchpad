SELECT
  T1.'FormattedDue' AS 'Month',
  COUNT(*) AS 'Count',
  'FormattedDue' AS 'Source'
FROM T1
GROUP BY T1.'FormattedDue'
ORDER BY T1.'FormattedDue'

========

SELECT
  T1.'FormattedResolution' AS 'Month',
  COUNT(*) AS 'Count',
  'FormattedResolution' AS 'Source'
FROM T1
GROUP BY T1.'FormattedResolution'
ORDER BY T1.'FormattedResolution'

========

SELECT * FROM T1
UNION ALL
SELECT * FROM T2
